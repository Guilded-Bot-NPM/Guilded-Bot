{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": ["src", "src/classes", "src/classes/client", "src/classes/Structures", "src/classes/Structures/User", "src/helper"],
        "exclude": [".vscode", "data\\", "dist", "doc", "node_modules", "test", "*.bak"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "encoding": "utf8",
        "private": true,
        "recurse": false,
        "readme": "./README.md",
        "destination": "./docs/",
        "theme_opts": {
            "theme": "dark"
        },
        "tutorials": "./tutorials"
    }
}